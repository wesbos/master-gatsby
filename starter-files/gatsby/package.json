{
  "name": "slicks-slices-frontend",
  "private": true,
  "description": "Slick Serves the Sweetest Slices",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "npx --node-arg '-r esm' gatsby build",
    "develop": "npx --node-arg '-r esm' gatsby develop",
    "start": "npm run develop",
    "serve": "npx --node-arg '-r esm' gatsby serve",
    "clean": "gatsby clean",
    "netlify": "netlify dev",
    "prebuild": "netlify-lambda install"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ]
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "gatsby": "^2.23.3",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-styled-components": "^3.3.4",
    "gatsby-source-sanity": "^5.0.6",
    "isomorphic-fetch": "^2.2.1",
    "netlify-cli": "^2.53.0",
    "netlify-lambda": "^1.6.3",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "prettier": "^2.0.5"
